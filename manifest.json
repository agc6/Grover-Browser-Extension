{
    "manifest_version": 3,
    "name": "Text Classifier",
    "description": "Detects whether text on a webpage is machine-generated or human-written.",
    "version": "1.0",
    "permissions": [
      "activeTab"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  