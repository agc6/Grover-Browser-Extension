{
    "manifest_version": 3,
    "name": "Grover",
    "description": "Detects whether text on a webpage is machine-generated or human-written.",
    "version": "1.0",
    "permissions": [
      "scripting",
      "activeTab",
      "storage"
    ],
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
    },
    "icons": {
      "16": "icons8-fake-16.png",
      "48": "icons8-fake-48.png",
      "128": "icons8-fake-128.png"
    },
    "action": {
      "default_popup": "popup.html"
    },

  "action": {
      "default_title": "Get pages source",
      "default_popup": "popup.html"
  },

  "permissions": [
      "scripting",
      "activeTab"
  ]
}