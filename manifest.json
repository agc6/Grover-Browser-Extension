{
    "manifest_version": 3,
    "name": "Grover",
    "description": "Detects whether text on a webpage is machine-generated or human-written.",
    "version": "1.0",
    "permissions": [
      "scripting",
      "tabs",
      "activeTab",
      "storage"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon":{
        "16": "icons8-fake-16.png",
        "48": "icons8-fake-48.png",
        "128": "icons8-fake-128.png"
      }
    },

    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  
  